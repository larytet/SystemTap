function _umountflags_str:string(op:long) 
%{ /* pure */
	int len, op = STAP_ARG_op;
	char *str = STAP_RETVALUE;
	if (op == 0)
		strlcpy(str,"0",MAXSTRINGLEN);
	else {
		str[0] = '\0';
		if (op & MNT_FORCE)
			strlcat(str,"MNT_FORCE|",MAXSTRINGLEN);
